{
    "manifest_version": 2,

    "name": "Mariner",
    "description": "This extension allows the user to change the background color of the comments.",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Mariner"
    },

    "permissions": [
        "activeTab",
        "storage"
    ]
}